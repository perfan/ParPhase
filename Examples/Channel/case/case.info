; Test case for the 2D Channel

CaseName Channel2D

Solver
{
	type "fractional step"
	timeStep 2.5e-3
	maxTime 20
	maxCo 1
}

LinearAlgebra
{
  uEqn
  {
    lib belos
  }

  pEqn
  {
    lib belos
    tolerance 1e-2
  }
}

Properties
{
	rho 1
	mu 1e-3
	g (0,0)
	gamma 0.001
}

Grid
{
	type rectilinear
	nCellsX 200
	nCellsY 100

	; Rectilinear parameters
	width 0.8
	height 0.4
        origin (0.1,0.1)
}

Viewer
{
	type tecplot
	integerFields procNo,proc
	vectorFields u,gradP
	scalarFields p,pCorr,m
}
